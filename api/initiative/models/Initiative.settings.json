{
  "connection": "default",
  "collectionName": "initiatives",
  "info": {
    "name": "initiative",
    "description": "Initiatives are projects that drive key results"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "activity": {
      "group": "activity",
      "repeatable": true,
      "type": "group"
    },
    "complete": {
      "type": "boolean",
      "default": false
    }
  }
}
